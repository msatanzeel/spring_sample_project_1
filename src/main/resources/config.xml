<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-2.5.xsd">


    <bean class = "com.spring.Student" name="student0">
        <property name="id">
            <value>101</value>
        </property>
        <property name="name">
            <value>Tanzeel</value>
        </property>
        <property name="address">
            <value>Delhi</value>
        </property>
    </bean>

    <!-- bean definitions here -->
    <bean class = "com.spring.Student" name="student1">
        <property name="id" value="101"/>
        <property name="address" value="Delhi"/>
        <property name="name" value="Tanzeel"/>
    </bean>

    <bean class="com.spring.Student" name="student2" p:address="Luknow" p:id="101" p:name="Tanzeel" />

</beans>